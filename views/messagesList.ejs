<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <a href="/new"><button onclick="">Create Message</button></a>

    <% messages.forEach((message) => { %>
    <div>
      <%- include('messageCard', {message:message}) %>
      <a href="/<%= message.id %>"><button>View Details</button></a>
    </div>
    <% }); %>
  </body>
</html>
